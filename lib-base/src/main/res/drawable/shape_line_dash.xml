<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <!-- 虚线 -->
    <!--
        使用的地方需要加android:layerType="software"
        线宽为dashWith，
        线之间空隙dashGap，d
        ashGap=0dp时，是实线
    -->
    <stroke
        android:width="1dp"
        android:color="#a6a6a6"
        android:dashGap="4dp"
        android:dashWidth="6dp" />
</shape>

